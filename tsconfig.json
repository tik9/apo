{
    "allowSyntheticDefaultImports": true,
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "skipLibCheck": true
    },
    "esModuleInterop": true,
    "exclude": [
        "node_modules",
        "./dist/**/*"
    ],
}